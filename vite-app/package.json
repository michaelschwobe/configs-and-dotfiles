{
  "name": "configs-and-dotfiles-vite-app",
  "version": "1.0.0",
  "private": true,
  "description": "Personal configurations and dotfiles for vite applications.",
  "keywords": [
    "configs",
    "dotfiles",
    "settings",
    "preferences",
    "vite"
  ],
  "homepage": "https://github.com/michaelschwobe/configs-and-dotfiles/vite-app",
  "repository": "github:michaelschwobe/configs-and-dotfiles",
  "license": "MIT",
  "author": {
    "name": "Michael Schwobe",
    "email": "mschwobe@gmail.com",
    "url": "https://schwobe.dev"
  },
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf coverage/ && rm -rf dist/ && rm -rf dist-ssr/ && rm -rf playwright-report/ && rm -rf test-results/ && rm -rf tests/prisma/ && rm -rf tsconfig.tsbuildinfo && rm -rf node_modules/.cache/",
    "dev": "vite",
    "format": "prettier --cache --cache-location ./node_modules/.cache/prettier --experimental-cli --ignore-unknown --log-level=warn --write .",
    "knip": "knip",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "nuke": "pnpm run clean && rm -rf node_modules",
    "preview": "vite preview",
    "setup": "pnpm install && pnpm run build",
    "typecheck": "tsc --build -noEmit",
    "validate": "pnpm run clean && pnpm run typecheck && pnpm run lint && pnpm run knip"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.7.1",
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@prettier/plugin-oxc": "^0.1.3",
    "@tailwindcss/vite": "^4.1.18",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "knip": "^5.81.0",
    "prettier": "^3.8.0",
    "prettier-plugin-packagejson": "^2.5.21",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-tsconfig-paths": "^6.0.4"
  },
  "engines": {
    "node": ">=24.13.0"
  }
}
